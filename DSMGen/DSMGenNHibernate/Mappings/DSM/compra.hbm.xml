<?xml version="1.0" encoding="utf-8" ?>
		<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
		                   assembly="DSMGenNHibernate"
		                   namespace="DSMGenNHibernate.EN.DSM">
		    
		    
			
				<class name="CompraEN" table="Compra"> 
				  	
						<id name="Id" column="idCompra" type="integer"> 
							<generator class="hilo"/>
						</id>
			
					
					<property name="Precio" type="integer"> 
						<column name="precio"  not-null="true"/>
					</property>
					<property name="EstadoCompra" > 
						<column name="estadoCompra"  not-null="true"/>
					</property>
			
			
			
		  			
						<bag name="Usuario" table="[usuario_compra]" inverse="true" >
							<key>
								
									<column name="FK_idCompra_idCompra"/> 
							</key>
      						<many-to-many class="DSMGenNHibernate.EN.DSM.UsuarioEN">
									<column name="FK_email_idUsuario"/> 
      						</many-to-many>
    					</bag>
			
			
					
					
					
					
		
		 </class>	
		  
		  <query name="CompraENReadAllHQL">from CompraEN</query>
		</hibernate-mapping>
	