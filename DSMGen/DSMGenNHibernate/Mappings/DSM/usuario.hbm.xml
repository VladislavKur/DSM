<?xml version="1.0" encoding="utf-8" ?>
		<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
		                   assembly="DSMGenNHibernate"
		                   namespace="DSMGenNHibernate.EN.DSM">
		    
		    
			
				<class name="UsuarioEN" table="Usuario"> 
						<id name="Email" column="email"/> 
			
					
					<property name="Pass" type="string"> 
						<column name="pass"  not-null="true"/>
					</property>
					<property name="Nombre_Real" type="string"> 
						<column name="nombre_Real"  not-null="true"/>
					</property>
					<property name="Apellido" type="string"> 
						<column name="apellido"  not-null="true"/>
					</property>
					<property name="Fecha_de_nacimiento" type="date"> 
						<column name="fecha_de_nacimiento"  not-null="true"/>
					</property>
					<property name="Orientacion_sexual" type="string"> 
						<column name="orientacion_sexual"  not-null="true"/>
					</property>
					<property name="Genero" type="boolean"> 
						<column name="genero"  not-null="true"/>
					</property>
					<property name="Nickname" type="string"> 
						<column name="nickname"  not-null="true"/>
					</property>
					<property name="Fecha_de_registro" type="date"> 
						<column name="fecha_de_registro"  not-null="true"/>
					</property>
					<property name="Likecounter" type="integer"> 
						<column name="likecounter"  not-null="true"/>
					</property>
			
			
			
			
			
			
			
			
			
			
			
			
		  			
						<bag name="Match_receptor" >
							<key>
							
								
								<column name="FK_email_idUsuario"/>
					      	</key>
							<one-to-many class="DSMGenNHibernate.EN.DSM.MatchEN"/>
					    </bag>
					
			
		  			
					
							<many-to-one name="Match_emisor" class="DSMGenNHibernate.EN.DSM.MatchEN" >
								<column name="FK_idMatch_idMatch_0"/> 
        					</many-to-one>
					 
						
			
		  			
						<bag name="Mensaje_receptor" >
							<key>
							
								
								<column name="FK_email_idUsuario"/>
					      	</key>
							<one-to-many class="DSMGenNHibernate.EN.DSM.MensajeEN"/>
					    </bag>
					
			
		  			
						<bag name="Mensaje_emisor" >
							<key>
							
								
								<column name="FK_email_idUsuario_0"/>
					      	</key>
							<one-to-many class="DSMGenNHibernate.EN.DSM.MensajeEN"/>
					    </bag>
					
			
		  			
						<bag name="Compra" table="[usuario_compra]"  >
							<key>
								
									<column name="FK_email_idUsuario"/> 
							</key>
      						<many-to-many class="DSMGenNHibernate.EN.DSM.CompraEN">
									<column name="FK_idCompra_idCompra"/> 
      						</many-to-many>
    					</bag>
			
		  			
						<many-to-one name="Sesion_activa" class="DSMGenNHibernate.EN.DSM.SesionEN" >	
								<column name="FK_idSesion_idSessionInicio"/> 
						</many-to-one>
											 					
			
		  			
						<bag name="Sesion_terminada" table="[usuario_0_sesion]"  >
							<key>
								
									<column name="FK_email_idUsuario_0"/> 
							</key>
      						<many-to-many class="DSMGenNHibernate.EN.DSM.SesionEN">
									<column name="FK_idSesion_idSesion"/> 
      						</many-to-many>
    					</bag>
					
					
					
					
					
					
					
					
					
					
					
					
					
					
					
					
					
		
		 </class>	
		  
		  <query name="UsuarioENReadAllHQL">from UsuarioEN</query>
		</hibernate-mapping>
	